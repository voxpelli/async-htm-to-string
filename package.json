{
  "name": "async-htm-to-string",
  "version": "0.1.0",
  "description": "Renders a htm tagged template asyncly into a string",
  "homepage": "http://github.com/voxpelli/async-htm-to-string",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/async-htm-to-string.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev -i type-fest",
    "check:installed-check": "installed-check -i eslint",
    "check:lint": "eslint .",
    "check:skypack": "package-check | echo \"\"",
    "check:tsc": "tsc",
    "check": "run-s clean && run-p check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts')",
    "clean": "run-p clean:*",
    "prepublishOnly": "run-s build",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s check test:*",
    "watch:build": "nodemon -x \"run-s build\"",
    "watch:tsd": "nodemon -e ts -x \"npx tsd\"",
    "watch": "run-p watch:*"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@sindresorhus/is": "^4.0.0",
    "htm": "^3.0.4",
    "stringify-entities": "^3.1.0"
  },
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^12.12.70",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@voxpelli/eslint-config": "6.0.0",
    "c8": "^7.3.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dependency-check": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "husky": "^4.3.0",
    "installed-check": "^3.0.0",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "tsd": "^0.13.1",
    "typescript": "^4.0.3"
  }
}
